    "extra": {
        "laravel": {
            "providers": [
                "Jrbarna\\UcrmLaravel\\UcrmLaravelServiceProvider"
            ],
            "aliases": {
                "SimplePayLaravel": "Jrbarna\\UcrmLaravel\\UcrmLaravelFacade"
            }
        }
    },
    "authors": [
        {
            "name": "jrbarna",
            "email": "jr.barna@gmail.com"
        }
    ],
    "minimum-stability": "dev"
