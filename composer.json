{
    "name": "jrbarna/ucrm-laravel",
    "version": "1.0.1",
    "description": "Simple package to interact with the ucrm api",
    "type": "library",
    "require": {
        "laravel/framework": ">=10"
    },
    "require-dev": {
        "orchestra/testbench": ">=9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jrbarna\\UcrmLaravel\\": "src/"
        }
